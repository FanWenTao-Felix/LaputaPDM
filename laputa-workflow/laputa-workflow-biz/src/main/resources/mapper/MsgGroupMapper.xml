<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.laputa.pdm.message.mapper.MsgGroupMapper">

  <resultMap id="msgGroupMap" type="com.laputa.pdm.message.api.entity.MsgGroup">
                  <id property="id" column="id"/>
                        <result property="createTime" column="create_time"/>
                        <result property="description" column="description"/>
                        <result property="name" column="name"/>
                        <result property="ownerId" column="owner_id"/>
                        <result property="picture" column="picture"/>
                        <result property="updateTime" column="update_time"/>
            </resultMap>
    <select id="findGroupByNameEquals" resultMap="msgGroupMap"></select>
    <select id="findGroupByNameLike" resultType="java.util.List"  resultMap="msgGroupMap"></select>

</mapper>
